{
  "manifest_version": 2,
  "name": "Blursday",
  "version": "1.0.0",
  "chrome_url_overrides": {
    "newtab": "new-tab.html"
  },
  "content_security_policy": "default-src 'self' https://blursday.app; script-src 'self'; object-src 'self'",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  }
}
